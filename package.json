{
  "name": "degree-picker",
  "private": false,
  "version": "0.0.8",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatijaNovosel/vue-degree-picker.git"
  },
  "keywords": [
    "degrees",
    "vue3",
    "vue",
    "vuejs",
    "degree-picker",
    "radial-picker",
    "radial"
  ],
  "main": "./dist/degree-picker.umd.js",
  "module": "./dist/degree-picker.es.js",
  "exports": {
    ".": {
      "import": "./dist/degree-picker.es.js",
      "require": "./dist/degree-picker.umd.js"
    },
    "./dist/degree-picker.css": {
      "import": "./dist/degree-picker.css",
      "require": "./dist/degree-picker.css"
    }
  },
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:types",
    "preview": "vite preview",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2"
  }
}
